{
  "name": "y-zustand",
  "version": "1.1.0",
  "description": "A simple and efficient middleware to synchronize a [Zustand](https://github.com/pmndrs/zustand) store with a [Yjs](https://github.com/yjs/yjs) document.",
  "type": "module",
  "main": "dist/y-zustand.js",
  "module": "dist/y-zustand.js",
  "types": "dist/y-zustand.d.ts",
  "exports": {
    ".": {
      "import": "./dist/y-zustand.js",
      "types": "./dist/y-zustand.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/y-zustand.ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "yjs",
    "zustand",
    "middleware",
    "sync",
    "realtime",
    "collaboration"
  ],
  "author": "Kit So",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/y-zustand.git"
  },
  "homepage": "https://github.com/your-username/y-zustand#readme",
  "peerDependencies": {
    "yjs": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}